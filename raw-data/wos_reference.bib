@article{ WOS:000706200200012,
Author = {Cotella, Giancarlo and Rivolin, Umberto Janin and Pede, Elena and
   Pioletti, Maurizio},
Title = {MULTI-LEVEL REGIONAL DEVELOPMENT GOVERNANCE: A EUROPEAN TYPOLOGY},
Journal = {EUROPEAN SPATIAL RESEARCH AND POLICY},
Year = {2021},
Volume = {28},
Number = {1},
Pages = {201-221},
Month = {JUN},
Abstract = {The European Union identifies the regional level as the ideal spatial
   scale for resources' redistiibution, in so doing turning European
   regions into key spatial development players. This raises challenges due
   to the heterogeneity of the EU in terms of administrative
   configurations, and spatial governance and planning systems. The
   contribution of this article draws on the results of three interlinked
   LSPON research projects to shed light on the matter. Building on an
   overview of the institutional variables that may influence successful
   regional development, it proposes a typology of multi-level regional
   development governance in the EU and reflects upon the potentials for
   delivering economic, social, and territorial cohesion.},
