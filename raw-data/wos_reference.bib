@article{ WOS:000989213300001,
Author = {Nzimande, Ntombifuthi Precious},
Title = {Stakeholders' perceptions of urban regeneration: the case of kis-pongrac
   in budapest},
Journal = {ENVIRONMENTAL RESEARCH COMMUNICATIONS},
