@article{ WOS:000241242800009,
Author = {Fagotto, Elena and Fung, Archon},
Title = {Empowered participation in urban governance: The Minneapolis
   Neighborhood Revitalization Program},
Journal = {INTERNATIONAL JOURNAL OF URBAN AND REGIONAL RESEARCH},
Year = {2006},
Volume = {30},
