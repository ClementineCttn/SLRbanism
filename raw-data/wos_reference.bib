@article{ WOS:000486094400014,
Author = {Silvestre, Hugo Consciencia and Marques, Rui Cunha and Dollery, Brian
   and Correia, Aldenisio Moraes},
Title = {Shared services in Brazilian local government: Urban development in
   small counties},
Journal = {PUBLIC ADMINISTRATION},
