@article{ WOS:000954367300001,
Author = {Ivars-Baidal, Josep and Casado-Diaz, Ana B. and Navarro-Ruiz, Sandra and
   Fuster-Uguet, Marc},
Title = {Smart tourism city governance: exploring the impact on stakeholder
   networks},
