@article{ WOS:001076105000001,
Author = {Sheffield, Connor and Valler, Dave},
Title = {`Dealing' with Governance and Planning? The Limits of Urban
   Intrapreneurialism},
Journal = {PLANNING THEORY \& PRACTICE},
Year = {2023},
Volume = {24},
Number = {4},
Pages = {453-472},
Month = {AUG 8},
Abstract = {This paper examines `deal-based' policy responses to local and
   sub-regional governance dilemmas, drawing on issues around strategic
   planning policy in Oxfordshire, UK. Deal-based policy is conceptualised
   as a form of urban intrapreneurialism, explicitly designed to cultivate
   change within local state operations and to promote associated
   organisational innovation, institutional proactivity, and policy
   reorientation. A general evaluative frame for urban intrapreneurialism
   is derived and then deployed for the Oxfordshire case, assessing the
   extent to which deal-based policy is able to respond to the distinctive
   and challenging set of governance dilemmas which pertain. Finally,
   broader conceptual and policy implications are discussed.},
